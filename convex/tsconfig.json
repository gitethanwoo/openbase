{
  "compilerOptions": {
    "allowJs": true,
    "strict": true,
    "target": "ESNext",
    "lib": ["ES2021", "DOM"],
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "noEmit": true,
    "isolatedModules": true
  },
  "include": ["./**/*"],
  "exclude": ["./_generated"]
}
